<?xml version="1.0" encoding="UTF-8"?>
<config>
	<nav>
		<library_home>
			<label>Library</label>
			<order>-100</order>
			<module>library</module>
			<controller>index</controller>
			<action>index</action>
		</library_home>
		<barcode>
			<label>Barcode</label>
			<uri>#</uri>
			<resource>library_barcode_accessno</resource>
<!-- <resource>library_authenticate_index</resource> -->
			<pages>
				<student>
					<label>Studdents' Roll</label>
					<module>library</module>
					<controller>barcode</controller>
					<action>student</action>
					<resource>library_barcode_student</resource>
				</student>
				<accessno>
					<label>Accession</label>
					<module>library</module>
					<controller>barcode</controller>
					<action>accessno</action>
					<resource>library_barcode_accessno</resource>
				</accessno>
				<isbn>
					<label>ISBN</label>
					<module>library</module>
					<controller>barcode</controller>
					<action>isbn</action>
					<resource>library_barcode_isbn</resource>
				</isbn>
			</pages>
		</barcode>
		<manage>
			<label>Manage</label>
			<uri>#</uri>
			<resource>library_isbn_index</resource>
			<pages>
				<isbn>
					<label>ISBN</label>
					<module>library</module>
					<controller>isbn</controller>
					<resource>library_isbn_index</resource>
				</isbn>
				<book>
					<label>Books</label>
					<module>library</module>
					<controller>book</controller>
					<resource>library_book_index</resource>
				</book>
			</pages>
		</manage>
		<transactions>
			<label>Transactions</label>
			<uri>#</uri>
			<resource>library_issue_index</resource>
			<pages>
				<issue>
					<label>Issue</label>
					<module>library</module>
					<controller>issue</controller>
					<resource>library_issue_index</resource>
				</issue>
				<return>
					<label>Return</label>
					<module>library</module>
					<controller>return</controller>
					<resource>library_return_index</resource>
				</return>
				<circulation>
					<label>Circulation</label>
					<module>library</module>
					<controller>book</controller>
					<action>circulation</action>
					<resource>library_book_circulation</resource>
				</circulation>
			</pages>
		</transactions>
		<opac>
			<label>OPAC</label>
			<uri>#</uri>
			<pages>
				<basicSearch>
					<label>Search</label>
					<module>library</module>
					<controller>search</controller>
					<resource>library_search_index</resource>
				</basicSearch>
				<gridSearch>
					<label>Grid Search</label>
					<module>library</module>
					<controller>search</controller>
					<action>grid</action>
					<resource>library_search_grid</resource>
				</gridSearch>
			</pages>
		</opac>
		<checkup>
			<label>Check ups</label>
			<uri>#</uri>
			<resource>library_opac_index</resource>
			<pages>
				<gate>
					<label>Gate</label>
					<module>library</module>
					<controller>gate</controller>
					<resource>library_gate_index</resource>
				</gate>
				<issueReturn>
					<label>Issue/Return</label>
					<module>library</module>
					<controller>opac</controller>
					<resource>library_opac_index</resource>
				</issueReturn>
			</pages>
		</checkup>
		<info>
			<label>Info</label>
			<uri>#</uri>
			<resource>library_member_accountinfo</resource>
			<pages>
				<member>
					<label>Member</label>
					<module>library</module>
					<controller>member</controller>
					<action>accountinfo</action>
					<resource>library_member_accountinfo</resource>
				</member>
			</pages>
		</info>
	</nav>
</config>